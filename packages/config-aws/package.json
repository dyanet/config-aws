{
  "name": "@dyanet/config-aws",
  "version": "1.0.1",
  "description": "Framework-agnostic AWS configuration management with support for environment variables, AWS Secrets Manager, SSM Parameter Store, S3, and .env files",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:types && npm run build:esm && npm run build:cjs",
    "build:types": "tsc --project tsconfig.types.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "configuration",
    "aws",
    "secrets-manager",
    "ssm",
    "parameter-store",
    "s3",
    "env-file",
    "typescript",
    "zod",
    "framework-agnostic"
  ],
  "author": "Dyanet",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "@aws-sdk/client-secrets-manager": "^3.0.0",
    "@aws-sdk/client-ssm": "^3.0.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "zod": "^3.22.0"
  },
  "peerDependenciesMeta": {
    "@aws-sdk/client-secrets-manager": {
      "optional": true
    },
    "@aws-sdk/client-ssm": {
      "optional": true
    },
    "@aws-sdk/client-s3": {
      "optional": true
    }
  },
  "devDependencies": {
    "@aws-sdk/client-secrets-manager": "^3.0.0",
    "@aws-sdk/client-ssm": "^3.0.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "aws-sdk-client-mock": "^4.0.0",
    "eslint": "^8.0.0",
    "fast-check": "^3.15.0",
    "jest": "^29.5.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.9.2",
    "zod": "^3.22.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dyanet/config-aws.git"
  },
  "bugs": {
    "url": "https://github.com/dyanet/config-aws/issues"
  },
  "homepage": "https://github.com/dyanet/config-aws/tree/main/packages/config-aws"
}
